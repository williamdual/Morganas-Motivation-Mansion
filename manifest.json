{
    "name": "Persona - Morgana's Motivation Mansion",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A test takers best friend",
    "icons": {
        "16": "assets/imgs/morgan_icon.png",
        "48": "assets/imgs/morgan_icon.png",
        "128": "assets/imgs/morgan_icon.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "mona.js"
            ]
        }
    ],
    "action": {
        "default_popup": "den.html",
        "default_title": "Thieves Den"
    },
    "options_page": "velvetRoom.html",
    "minimum_chrome_version": "43.0"
}